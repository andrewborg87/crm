{
  "$schema": "https://www.krakend.io/schema/v3.json",
  "version": 3,
  "endpoints": [
    {
      "backend": [
        {
          "method": "GET",
          "url_pattern": "/docs-json",
          "host": ["http://broker:3000"]
        }
      ],
      "method": "GET",
      "endpoint": "/broker/docs-json"
    },
    {
      "backend": [
        {
          "method": "GET",
          "url_pattern": "/docs/{any}",
          "host": ["http://broker:3000"]
        }
      ],
      "method": "GET",
      "endpoint": "/broker/docs/{any}",
      "output_encoding": "no-op"
    },
    {
      "backend": [
        {
          "method": "GET",
          "url_pattern": "/docs",
          "host": ["http://broker:3000"]
        }
      ],
      "method": "GET",
      "endpoint": "/broker/docs",
      "output_encoding": "no-op"
    },
    {
      "backend": [
        {
          "method": "GET",
          "url_pattern": "/api/health/prometheus",
          "host": ["http://broker:3000"]
        }
      ],
      "method": "GET",
      "endpoint": "/api/health/prometheus",
      "output_encoding": "no-op"
    },
    {
      "backend": [
        {
          "method": "GET",
          "url_pattern": "/api/health",
          "host": ["http://broker:3000"]
        }
      ],
      "method": "GET",
      "endpoint": "/api/health",
      "output_encoding": "no-op"
    },
    {
      "backend": [
        {
          "method": "GET",
          "url_pattern": "/api/health/ping",
          "host": ["http://broker:3000"]
        }
      ],
      "method": "GET",
      "endpoint": "/api/health/ping",
      "output_encoding": "no-op"
    }
  ]
}
